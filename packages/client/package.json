{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "config-overrides-path": "config-overrides.cjs",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "react-app-rewired build",
    "start:dev": "react-app-rewired start",
    "start:debug": "yarn start",
    "format": "prettier --write src/",
    "lint": "eslint --cache src/",
    "lint:fix": "eslint --fix src/",
    "slint": "stylelint --cache --allow-empty-input src/",
    "slint:fix": "stylelint --fix --allow-empty-input src/",
    "test": "jest --passWithNoTests",
    "test:staged": "yarn test --no-cache --bail=1",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "pre-commit": "lint-staged",
    "pre-push": "yarn lint:fix && yarn slint:fix && yarn test --bail=1 && yarn format"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.1",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.63.3",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  }
}
