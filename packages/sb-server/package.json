{
  "name": "sb-server",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "nest build",
    "start": "SET NODE_ENV=dev& nest start",
    "start:dev": "SET NODE_ENV=dev& nest start --watch",
    "start:debug": "SET NODE_ENV=dev& nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write src/",
    "lint": "eslint --cache src/",
    "lint:fix": "eslint --fix src/",
    "test": "SET NODE_ENV=test& jest --passWithNoTests",
    "test:watch": "SET NODE_ENV=test& yarn test --watch",
    "test:coverage": "SET NODE_ENV=test& yarn test --watch --coverage",
    "pre-commit": "lint-staged",
    "pre-push": "lint-staged"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "^7.1.7",
    "@nestjs/typeorm": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mysql2": "^3.5.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig": "^7.0.0"
  }
}
